	<lyric>
		<block type="verse">
			<row important_over="true">Ch<ch a="a"/>orałem dzwonków dz<ch a="d"/>ień rozkw<ch a="E"/>ita</row>
			<row important_over="true">J<ch a="a"/>eszcze od rosy rz<ch a="F"/>ęsy m<ch a="G"/>okre</row>
			<row important_over="true">W<ch a="C"/>e mgle turkoce pi<ch a="d"/>erwsza bryka</row>
			<row important_over="true">Sł<ch a="E"/>ońce wyrusza n<ch a="E7"/>a włóczęgę</row>
		</block>
		<block type="chorus">
			<row important_over="true">Dr<ch a="a"/>ogą pylistą, dr<ch a="d"/>ogą p<ch a="E"/>olną</row>
			<row important_over="true">J<ch a="a"/>ak kolorowa p<ch a="F"/>anny kr<ch a="G"/>ajka</row>
			<row important_over="true">Sł<ch a="C"/>ońce się wznosi n<ch a="d"/>ad stodołą</row>
			<row important_over="true">B<ch a="E"/>ędzie tańczyć walc<ch a="E7"/>a</row>
			<bis>
				<row important_over="true">
					<ch a="F"/>A ja mam sw<ch a="G"/>oją gitarę</row>
				<row important_over="true">Sp<ch a="C"/>odnie wytarte i b<ch a="a"/>uty stare</row>
				<row important_over="true">W<ch a="d"/>iatry n<ch a="E"/>iosą mn<ch a="a"/>ie na skrz<ch a="A7"/>ydłach</row>
			</bis>
		</block>
		<block type="verse">
			<row important_over="false">Zm<ch a="a"/>oknięte świerszcze str<ch a="d"/>oją skrz<ch a="E"/>ypce</row>
