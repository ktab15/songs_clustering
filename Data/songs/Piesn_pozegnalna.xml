<?xml version="1.0" encoding="utf-8"?>
<song xmlns="http://21wdh.staszic.waw.pl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://21wdh.staszic.waw.pl Song.xsd" title="Pieśń pożegnalna">
  <alias/>
  <text_author/>
  <comment/>
  <composer type="solo"/>
  <artist type="solo"/>
  <translator/>
  <original_title/>
  <lyric>
    <block type="verse">
      <row important_over="false"><ch a="D"/>Ogniska już dogasa blask<ch a="A"/></row>
      <row important_over="false"><ch a="D"/>Braterski splećmy krąg<ch a="G"/></row>
      <row important_over="false"><ch a="D"/>W wieczornej ciszy, w świetle gwiazd<ch a="A"/></row>
      <row important_over="false"><ch a="D"/>Ostatni uścisk rąk<ch a="G"/><ch a="D"/></row>
    </block>
    <block type="chorus">
      <row important_over="false"><ch a="D"/>Kto raz przyjaźni poznał moc<ch a="A"/></row>
      <row important_over="false"><ch a="D"/>Nie będzie trwonić słów<ch a="G"/></row>
      <row important_over="false"><ch a="D"/>Przy innym ogniu w inną noc<ch a="A"/></row>
      <row important_over="false"><ch a="D"/>Do zobaczenia znów<ch a="G"/><ch a="D"/></row>
    </block>
    <block type="verse">
      <row important_over="false"><ch a="D"/>Nie zgaśnie tej przyjaźni żar<ch a="A"/></row>
      <row important_over="false"><ch a="D"/>Co połączyła nas<ch a="G"/></row>
      <row important_over="false"><ch a="D"/>Nie pozwolimy by ją starł<ch a="A"/></row>
      <row important_over="false"><ch a="D"/>Nieubłagany czas<ch a="G"/><ch a="D"/></row>
    </block>
    <blocklink blocknb="2"/>
  </lyric>
  <music metre="">
    <guitar barre="2"/>
  </music>
